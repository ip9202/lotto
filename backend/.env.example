# 로또리아 AI - 개발 환경 설정 파일
# 실제 사용 시 .env 파일로 복사해서 사용하세요

# 기본 환경 설정
ENVIRONMENT=development
DEBUG=true

# 데이터베이스 설정 (Docker 컨테이너 사용)
DATABASE_URL=postgresql://lotto_user:lotto_password@postgres:5432/lotto_db

# API 설정
API_HOST=0.0.0.0
API_PORT=8000

# JWT 보안 설정
SECRET_KEY=dev-secret-key-change-in-production-2024
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# 카카오 소셜 로그인 (개발용)
KAKAO_CLIENT_ID=your_kakao_client_id_here
KAKAO_CLIENT_SECRET=your_kakao_client_secret_here

# 네이버 소셜 로그인 (개발용)  
NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here

# CORS 설정
CORS_ORIGINS=http://localhost:5173,https://lottoria.ai.kr,https://www.lottoria.ai.kr